<%- include("partials/header.ejs") -%>

<%- include("partials/sidebar.ejs") -%>

<div class="col-sm-10">
  <div class="row">
    <div class="col-12 shadow-sm"><h2>Master Data</h2></div>
    <div class="col-12 shadow-sm">Vendor</div>
  </div>
<!-- container -->
<div class="container-fluid">
    
    <div class="row main-box bg-light rounded-3">
      <nav>
        <div class="nav nav-tabs" id="nav-tab" role="tablist">
          <button class="nav-link active" id="nav-home-tab" data-bs-toggle="tab" data-bs-target="#nav-home" type="button" role="tab" aria-controls="nav-home" aria-selected="true">Create</button>
          <button class="nav-link" id="nav-profile-tab" data-bs-toggle="tab" data-bs-target="#nav-profile" type="button" role="tab" aria-controls="nav-profile" aria-selected="false">Modify</button>
        </div>
      </nav>
      <div class="tab-content " id="nav-tabContent">
         <!-- first tab -->
         <div class="tab-pane fade show active " id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
          <form action="" method="post">
          <div class="row">
              
                <!-- 1st col -->
                <div class="col-sm-6">
                  <div class="form-group row mb-3">
                    <label class="col-sm-3 col-form-label">Vendor Code</label>
                    <div class="col-sm-4">
                      <input type="text" class="form-control" name="vd_code" required>
                    </div>
                  </div>
                  <div class="form-group row mb-3">
                    <label class="col-sm-3 col-form-label">Vendor Name</label>
                    <div class="col-sm-9">
                      <input type="text" class="form-control" name="vd_code" required>
                    </div>
                  </div>
                  <div class="form-group row mb-3">
                    <label class="col-sm-3 col-form-label">Company Code</label>
                    <div class="col-sm-4">
                    <select class="form-select" name="c_code">
                      <option selected>Choose...</option>
                      <option value="1">One</option>
                      <option value="2">Two</option>
                    </select>
                    </div>
                  </div>

                  <p class="desc border-bottom">Address</p>
                  <div class="form-group row mb-3">
                    <label class="col-sm-2 col-form-label">Street</label>
                    <div class="col-sm-9">
                      <input type="text" class="form-control" name="vd_street" required>
                    </div>
                  </div>
                  <div class="form-group row mb-3">
                    <label class="col-sm-2 col-form-label">Postal Code</label>
                    <div class="col-sm-3">
                      <input type="text" class="form-control" name="vd_postalcode" required>
                    </div>
                    <label class="col-sm-1 col-form-label">City</label>
                    <div class="col-sm-5">
                      <input type="text" class="form-control" name="vd_postalcode" required>
                    </div>
                  </div>
                  <div class="form-group row mb-3">
                    <label class="col-sm-2 col-form-label">Country</label>
                    <div class="col-sm-4">
                    <select class="form-select" name="vd_country">
                      <option selected disabled>Choose...</option>
                     <%  countries.forEach(function(country) { %>
                        <option value="<%= country %> "><%= country %> </option>
                     <%  }); %>
                    </select>
                    </div>
                  </div>
                </div>
                <!-- end of 1st col -->
                <!-- 2nd col -->
                <div class="col-sm-6">
                  <div class="form-group row mb-3">
                    <label class="col-sm-3 col-form-label">Tax</label>
                    <div class="col-sm-9">
                      <input type="text" class="form-control" name="vd_tax" required>
                    </div>
                  </div>
                  <div class="form-group row mb-3">
                    <label class="col-sm-3 col-form-label">Account Currency</label>
                    <div class="col-sm-9">
                    <select class="form-select" name="acc_currency">
                      <option selected>Choose...</option>
                      <option value=""></option>
                      <option value="">Two</option>
                    </select>
                  </div>
                  </div>
                  <div class="form-group row mb-3">
                    <label class="col-sm-3 col-form-label">Tax Category</label>
                    <div class="col-sm-9">
                    <select class="form-select" name="tax_category">
                      <option selected>Choose...</option>
                      <option value=""></option>
                      <option value="">Two</option>
                    </select>
                  </div>
                  </div>
                  <div class="form-group row mb-3">
                    <label class="col-sm-3 col-form-label">Recon Acct for Acct Type</label>
                    <div class="col-sm-9">
                    <select class="form-select" name="tax_category">
                      <option selected>Choose...</option>
                      <option value=""></option>
                      <option value="">Two</option>
                    </select>
                  </div>
                  </div> 
                </div>
                <!-- end of 2nd col -->
                <!-- button -->
                
                <div class="container-fluid mb-3 d-flex flex-column">
                  <button type="button" class="btn btn-primary btn-lg icenter">Create</button>
                </div> <!-- end of button -->
               
             </div>  <!-- end of row -->
            </form>
        </div>
        <!-- end of first tab -->

        <!-- second tab -->
        <div class="tab-pane fade" id="nav-profile" role="tabpanel" aria-labelledby="nav-profile-tab">
          <form action="" method="post">
          <div class="row">
                <!-- 1st col -->
                <div class="col-sm-6">
                    <div class="form-group row mb-3">
                      <label class="col-sm-3 col-form-label">Vendor Code</label>
                      <div class="col-sm-4">
                        <input type="text" class="form-control" name="vd_code" required>
                      </div>
                    </div>
                    <div class="form-group row mb-3">
                      <label class="col-sm-3 col-form-label">Vendor Name</label>
                      <div class="col-sm-9">
                        <input type="text" class="form-control" name="vd_name" required>
                      </div>
                    </div>
                    <div class="form-group row mb-3">
                      <label class="col-sm-3 col-form-label">Company Code</label>
                      <div class="col-sm-4">
                      <select class="form-select" name="c_code">
                        <option selected>Choose...</option>
                        <option value="1">One</option>
                        <option value="2">Two</option>
                      </select>
                      </div>
                    </div>
  
                    <p class="desc border-bottom">Address</p>
                    <div class="form-group row mb-3">
                      <label class="col-sm-2 col-form-label">Street</label>
                      <div class="col-sm-9">
                        <input type="text" class="form-control" name="vd_street" required>
                      </div>
                    </div>
                    <div class="form-group row mb-3">
                      <label class="col-sm-2 col-form-label">Postal Code</label>
                      <div class="col-sm-3">
                        <input type="text" class="form-control" name="vd_postalcode" required>
                      </div>
                      <label class="col-sm-1 col-form-label">City</label>
                      <div class="col-sm-5">
                        <input type="text" class="form-control" name="vd_postalcode" required>
                      </div>
                    </div>
                    <div class="form-group row mb-3">
                      <label class="col-sm-2 col-form-label">Country</label>
                      <div class="col-sm-4">
                      <select class="form-select" name="vd_country">
                        <option selected disabled>Choose...</option>
                       <%  countries.forEach(function(country) { %>
                          <option value="<%= country %> "><%= country %> </option>
                       <%  }); %>
                      </select>
                      </div>
                    </div>
                  </div>
                  <!-- end of 1st col -->
                  <!-- 2nd col -->
                  <div class="col-sm-6">
                    <div class="form-group row mb-3">
                      <label class="col-sm-3 col-form-label">Tax</label>
                      <div class="col-sm-9">
                        <input type="text" class="form-control" name="vd_tax" required>
                      </div>
                    </div>
                    <div class="form-group row mb-3">
                      <label class="col-sm-3 col-form-label">Account Currency</label>
                      <div class="col-sm-9">
                      <select class="form-select" name="acc_currency">
                        <option selected>Choose...</option>
                        <option value=""></option>
                        <option value="">Two</option>
                      </select>
                    </div>
                    </div>
                    <div class="form-group row mb-3">
                      <label class="col-sm-3 col-form-label">Tax Category</label>
                      <div class="col-sm-9">
                      <select class="form-select" name="tax_category">
                        <option selected>Choose...</option>
                        <option value=""></option>
                        <option value="">Two</option>
                      </select>
                    </div>
                    </div>
                    <div class="form-group row mb-3">
                      <label class="col-sm-3 col-form-label">Recon Acct for Acct Type</label>
                      <div class="col-sm-9">
                      <select class="form-select" name="tax_category">
                        <option selected>Choose...</option>
                        <option value=""></option>
                        <option value="">Two</option>
                      </select>
                    </div>
                    </div> 
                  </div>
                  <!-- end of 2nd col -->   
                <!-- button -->
                <div class="container-fluid mb-3 d-flex flex-column">
                  <button type="button" class="btn btn-primary btn-lg icenter">Update</button>
                </div> <!-- end of button -->
             </div>  <!-- end of row -->
            </form>
        </div>
        <!-- end of second tab -->
      </div>

        
        


</div> 
<!-- end of container -->



<%- include("partials/footer.ejs") -%>
